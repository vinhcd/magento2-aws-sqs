<?xml version="1.0"?>

<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="vinhcd_aws_sqs_queue" resource="default" engine="innodb" comment="Customer Entity">
        <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true"
                comment="Entity ID"/>
        <column xsi:type="varchar" name="queue_url" length="255" nullable="false" comment="Queue URL"/>
        <column xsi:type="text" name="message" nullable="false" comment="Message"/>
        <column xsi:type="timestamp" name="created_at" on_update="false" nullable="true" comment="Creation time"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
    </table>
</schema>
